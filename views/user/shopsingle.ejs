<%- include('navbar') -%>
    
<style>
    .image-gallery {
        display: flex;
        flex-direction: row;
        overflow-x: scroll; /* Enables horizontal scrolling */
        gap: 55px; /* Adds space between images */
    }

    .image-gallery img {
        height: 100px;
        width: 100px;
        display: block;
    }

    /* Optional: styling for scrollbar */
    .image-gallery::-webkit-scrollbar {
        height: 10px;
    }

    .image-gallery::-webkit-scrollbar-thumb {
        background: #888;
        border-radius: 5px;
    }

    .image-gallery::-webkit-scrollbar-thumb:hover {
        background: #555;
    }
</style>
   

