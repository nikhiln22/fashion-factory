                            <div class="image-gallery">
                                <% if (singleProduct.image && singleProduct.image.length > 0) { %>
                                    <% singleProduct.image.forEach(function(image, index) { %>
                                        <div class="thumbnail"
                                        data-url="/<%= image %>">
                                            <!-- <p><%= index %></p> -->
                                            <img src="/<%= image %>" alt="no image">
                                        </div>
                                    <% }); %>
                                <% } else { %>
                                    <div>No images available</div>
                                <% } %>
                            </div>
                        </div>
                    </div>  
                   
